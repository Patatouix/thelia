<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="antispam.configuration" path="/admin/module/antispam/configuration/update" methods="POST">
        <default key="_controller">AntiSpam\Controller\Configuration::editConfiguration</default>
    </route>

    <route id="antispam.fake-validation" path="/module/antispam/contact/fake-validation" methods="GET">
        <default key="_controller">AntiSpam\Controller\ContactController::fakeValidation</default>
        <default key="_view">fake_validation</default>
    </route>

</routes>
